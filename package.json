{
  "name": "dp-vuecli",
  "author": "daipeng7",
  "version": "1.0.0",
  "description": "vue cli",
  "license": "ISC",
  "homepage": "https://github.com/daipeng7/dp-vuecli",
  "repository": {
    "type": "git",
